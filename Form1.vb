    Public List_Radio_Client_Health As List(Of String)
    Public Sub Radio_Checker()
        Dim Client_Health_RadioB = New List(Of String)
        Dim IsChecked As Boolean = True
        Select Case IsChecked
            Case RadioButton1.Checked
                Client_Health_RadioB.Add(RadioButton1.Text)
            Case RadioButton2.Checked
                Client_Health_RadioB.Add(RadioButton2.Text)
            Case RadioButton2.Checked
                Client_Health_RadioB.Add(RadioButton3.Text)
            Case RadioButton3.Checked
                Client_Health_RadioB.Add(RadioButton3.Text)
            Case RadioButton4.Checked
                Client_Health_RadioB.Add(RadioButton4.Text)
            Case RadioButton5.Checked
                Client_Health_RadioB.Add(RadioButton5.Text)
            Case RadioButton6.Checked
                Client_Health_RadioB.Add(RadioButton6.Text)
            Case RadioButton7.Checked
                Client_Health_RadioB.Add(RadioButton7.Text)
            Case RadioButton8.Checked
                Client_Health_RadioB.Add(RadioButton8.Text)
            Case RadioButton9.Checked
                Client_Health_RadioB.Add(RadioButton9.Text)
        End Select
        If (TextBox6.Text IsNot Nothing) Then Client_Health_RadioB.Add(TextBox6.Text)
        List_Radio_Client_Health = Client_Health_RadioB
    End Sub
    Private Sub Button1_Click(sender As Object, e As EventArgs) Handles Button1.Click
        'Update
        Dim Client_Info As Client_Info = New Client_Info()
        Dim Client_Health As Client_Health = New Client_Health()
        Dim Family_Health As Family_History_Information = New Family_History_Information()
        'Client Basic Information
        Dim Client(4) As String
        Client(0) = TextBox1.Text
        Client(1) = TextBox3.Text
        Client(2) = TextBox2.Text
        Client(3) = TextBox4.Text
        Client(4) = MonthCalendar1.SelectionRange.Start.ToString()
        Client_Info.Client_info = Client
        'Client Health Information
        Radio_Checker()
        Dim Client_health_info(3) As String
        Client_health_info(0) = RichTextBox1.Text
        Client_health_info(1) = RichTextBox2.Text
        Client_health_info(2) = MonthCalendar1.SelectionRange.Start.ToString()
        Client_Health.Client_Health = Client_health_info
        Client_Health.List_Radio_Client_Health = List_Radio_Client_Health
        'Family History Information
        Dim Family_History_Father(3) As String
        Family_History_Father(0) = TextBox7.Text
        Family_History_Father(1) = TextBox10.Text
        Family_History_Father(2) = TextBox13.Text
        Dim Family_History_Mother(3) As String
        Family_History_Mother(0) = TextBox8.Text
        Family_History_Mother(1) = TextBox11.Text
        Family_History_Mother(2) = TextBox14.Text
        Dim Family_History_Sister(3) As String
        Family_History_Sister(0) = TextBox16.Text
        Family_History_Sister(1) = TextBox17.Text
        Family_History_Sister(2) = TextBox18.Text
        Dim Family_History_Brother(3) As String
        Family_History_Brother(0) = TextBox9.Text
        Family_History_Brother(1) = TextBox12.Text
        Family_History_Brother(2) = TextBox15.Text
        Dim Family_History_Children(3) As String
        Family_History_Children(0) = TextBox19.Text
        Family_History_Children(1) = TextBox20.Text
        Family_History_Children(2) = TextBox21.Text
        Family_Health.Family_History_Father = Family_History_Father
        Family_Health.Family_History_Mother = Family_History_Mother
        Family_Health.Family_History_Sister = Family_History_Sister
        Family_Health.Family_History_Brother = Family_History_Brother
        Family_Health.Family_History_Children = Family_History_Children



        Client_Info.Clients()
        Client_Health.Client_Health_Info()
    End Sub

